{
  "name": "developer-showcase-manager",
  "version": "0.1.0",
  "private": true,
  "packageManager": "npm@10.2.4",
  "workspaces": [
    "welcome-nextjs",
    "portfolio-nextjs"
  ],
  "scripts": {
  "dev": "npm run serve",
  "dev:welcome": "cd welcome-nextjs && npm run dev", 
  "dev:portfolio": "cd portfolio-nextjs && npm run dev", 
  "dev:all": "npm run serve & npm run dev:welcome & npm run dev:portfolio",
  "build": "turbo build",
  "serve": "vue-cli-service serve --skip-plugins @vue/cli-cli-plugin-eslint --port 8080",
  "build:vue": "vue-cli-service build",
  "build:railway": "NODE_ENV=production vue-cli-service build",
  "serve:static": "npx serve dist -s -l $PORT",
  "start": "npx serve dist -s -p $PORT",
  "lint": "turbo lint"
},
  "dependencies": {
    "-": "^0.0.1",
    "@anthropic-ai/claude-code": "^1.0.43",
    "@supabase/supabase-js": "^2.50.0",
    "bcryptjs": "^3.0.2",
    "core-js": "^3.8.3",
    "serve": "^14.2.3",
    "vue": "^3.2.13",
    "vue-router": "^4.0.3",
    "vuex": "^4.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.12.16",
    "@babel/eslint-parser": "^7.12.16",
    "@vue/cli-plugin-babel": "~5.0.0",
    "@vue/cli-plugin-eslint": "~5.0.0",
    "@vue/cli-plugin-router": "~5.0.0",
    "@vue/cli-plugin-vuex": "~5.0.0",
    "@vue/cli-service": "~5.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-vue": "^8.0.3",
    "prettier": "^2.4.1",
    "sass": "^1.32.7",
    "sass-loader": "^12.0.0",
    "turbo": "^2.5.5"
  }
}
